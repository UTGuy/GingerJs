<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>GingerJs - Knockout IOC Framework</title>
        <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        
        <script type="text/javascript">
            (function () {
                
                // setup a namespace ( 'this' refers to 'window' while in a browser )
                this.t33 = {
                    models: {},
                    i18n: {},
                    urls: {}
                };
                
            })();
        </script>

        <script src="~/Scripts/jquery-1.8.2.min.js"></script>
        <script src="~/Scripts/jquery-ui-1.8.24.js"></script>
        <script src="~/Scripts/modernizr-2.6.2.js"></script>
        <script src="~/Scripts/knockout-2.2.0.js"></script>
        <script src="~/Scripts/knockout.mapping-latest.js"></script>
        <script src="~/Scripts/ginger.js"></script>
        <script src="~/Scripts/ginger.mapping.js"></script>
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        @RenderBody()

        @* footer *@
        <script type="text/javascript">
            Ginger.settings({ pageContainer: "#MyRoot" });
            Ginger.runEngine();
        </script>

    </body>
</html>